{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "default",
    "description": "Default permissions for CalendSync",
    "windows": ["main"],
    "permissions": [
        "core:default",
        "http:default",
        {
            "identifier": "http:default",
            "allow": [
                { "url": "http://localhost:3000/**" },
                { "url": "https://*.calendsync.app/**" }
            ]
        }
    ]
}
